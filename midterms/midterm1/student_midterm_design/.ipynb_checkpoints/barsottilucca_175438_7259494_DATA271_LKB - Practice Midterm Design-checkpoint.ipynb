   "source": [
    "Problem: Write a function that takes in a list and returns the list in reverse.\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "36316953-fb25-457f-8291-220f25f1feb5",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "['h', 'g', 'f', 'e', 'd', 'c', 'b', 'a']"
      ]
     },
     "execution_count": 5,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "list = ['a','b','c','d','e','f','g','h']\n",
    "ret = []\n",
    "i = len(list) - 1\n",
    "while i >= 0:\n",
    "    ret.append(list[i]);\n",
    "    i -= 1;\n",
    "ret"
   ]
  }
